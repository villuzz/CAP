        "credentials": {
          "destination": "Enel_Gateway"
        }
      },
      "ZEWRKR_NOTIFICATION_SRV": {
        "kind": "odata-v2",
        "model": "srv/external/ZEWRKR_NOTIFICATION_SRV",
